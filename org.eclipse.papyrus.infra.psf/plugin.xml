<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipse.papyrus.infra.psf.handler.ImportPSFHandler"
            description="Imports a Team Project Set File (Psf) in background"
            id="org.eclipse.papyrus.infra.psf.import"
            name="Import PSF (Background)">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
         <command
               commandId="org.eclipse.papyrus.infra.psf.import"
               style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IFile">
                           <test
                                 property="org.eclipse.core.resources.extension"
                                 value="psf">
                           </test>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
         </command>
      </menuContribution>
   </extension>

</plugin>
